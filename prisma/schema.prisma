
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id                        String          @id @default(uuid())
  username                  String          @unique
  isComplete                Boolean         @default(false)
  email                     String          @unique
  password                  String 
  section                   String
  rollNumber                String   
  rank                      Int             @default(0)    
  githubUsername            String?         @unique
  projectRating             Int?            
  leetcodeUsername          String          @unique
  codeforcesUsername        String          @unique
  projects                  Project[]
}

 
model Project {
  id                        String          @id @default(uuid())
  title                     String
  description               String       
  projectUrl                String
  liveLink                  String          
  userId                    String
  user                      User            @relation(fields: [userId], references: [id])
}
